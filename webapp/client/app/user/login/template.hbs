{{#modal-prototype containerClassNames="login-modal" close="transitionBack"}}
<div {{action 'transitionBack'}} class="close-modal">
	<svg viewBox="0 0 100 100" width="22" height="22" xmlns="http://www.w3.org/2000/svg">
	 <g>
	  <line id="svg_1" y2="90" x2="90" y1="10" x1="10" stroke-width="20" fill="none"/>
	  <line id="svg_2" y2="90" x2="10" y1="10" x1="90" stroke-width="20" fill="none"/>
	 </g>
	</svg>
</div>
<img src="images/logo.png" alt="">
<h2>Welcome back!</h2>
<h3>Log in to your account here!</h3>
{{#if alert}}
	<div class="alert alert-{{alert.type}}">{{alert.message}}</div>
{{/if}}
<form {{action "login" on="submit" }} class="login-form {{if isLoading 'au-loading'}}">
	<div class="email">{{input type="email" placeholder="Email address" value=email}}

